{
  "name": "starter-vue-vuex-scss",
  "description": "A lightweight Vue.js starter including Vuex, Vue Router and Axios.",
  "version": "1.0.0",
  "main": "src/main.js",
  "license": "MIT",
  "author": "Quentin Raimbault <quentin.raimbault@gmail.com>",
  "sassLintConfig": ".sass-lint.yml",
  "repository": {
    "url": "https://github.com/QRaimbault/vue-js-starter-scss",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/QRaimbault/vue-js-starter-scss/issues",
    "email": "quentin.raimbault@gmail.com"
  },
  "homepage": "https://qraimbault.github.io/vue-js-starter-scss/#/",
  "keywords": [
    "vue",
    "vuejs",
    "starter",
    "vuex",
    "axios-vue",
    "sass",
    "scss",
    "vue-router",
    "vuejs2",
    "vue2",
    "jest",
    "scss-styles"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development node_modules/.bin/webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack",
    "serve": "cross-env NODE_ENV=production node bin/spa-server.js",
    "lint-js": "eslint --ext .js,.vue src",
    "lint-js-fix": "eslint --ext .js,.vue src --fix",
    "lint-scss": "sass-lint -c ./.sass-lint.yml",
    "unit": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "\\.(css|jpg|jpeg|png|scss|sass|svg|eot|woff|woff2|ttf|otf|ico)$": "<rootDir>/__test__/empty-module.js"
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "vue": "^2.6.10",
    "vue-analytics": "^5.17.2",
    "vue-cookie": "^1.1.4",
    "vue-router": "^3.1.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "open": "^6.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "regenerator-runtime": "^0.13.3",
    "resolve-url-loader": "^3.1.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "sugarss": "^2.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "vue-eslint-parser": "^6.0.4",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}
